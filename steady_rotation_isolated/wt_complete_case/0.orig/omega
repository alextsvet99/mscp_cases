FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      omega;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 -1 0 0 0 0];

internalField uniform 1.60;

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           $internalField;
    }
    outlet
    {
        type            inletOutlet;
        inletValue      $internalField;
        value           $internalField;
    }
    sides
    {
        type            zeroGradient;
    }
    rotor
    {
        type            omegaWallFunction;
        value           uniform 1460;
    }
    ami_in
    {
        type            cyclicAMI;
    }
    ami_out
    {
        type            cyclicAMI;
    }
}

// ************************************************************************* //
